	"tokenColors": [
			{
				"name": "Comments",
				"scope": [
					"comment",
					"punctuation.definition.comment",
					"unused.comment",
					"wildcard.comment"
				],
				"settings": {
					"foreground": "#6272A4"
				}
			},
			{
				"name": "Function names",
				"scope": [
					"entity.name.function",
					"meta.function-call.generic",
					"meta.function-call.object",
