{
	"name": "Nix",
	"type": "dark",
	"colors": {
		// Basic Colors:
		"focusBorder":"#22A5F1",
		"foreground":"#808080",
		"icon.foreground":"#808080",
		"widget.shadow": "#000",
		"descriptionForeground":"#6216f0",

		// Text Colors:
		"textCodeBlock.background":"#1A1A1A",
		"textLink.foreground":"#22A5F1",

		// Button Control:
		"button.background":"#22A5F1",
		"button.foreground": "#FAFAFA",
		"button.hoverBackground":"#22A5F140",

		// Dropdown Control:
		"dropdown.background": "#1A1A1A",
		"dropdown.foreground": "#FAFAFA",
		"dropdown.listBackground": "#1A1A1A",
		"dropdown.border": "#808080",

		// Input Control:
		"input.background": "#1A1A1A",
		"input.foreground": "#808080",
		"input.border":"#808080",

		// Badge:
		"badge.background": "#22A5F1",
		"badge.foreground": "#FAFAFA",

		// Progress Bar:
		"progressBar.background": "#1A1A1A",

		// List And Trees:
		"list.activeSelectionBackground":"#22A5F140",
		"list.activeSelectionForeground":"#FAFAFA",
		"list.focusBackground":"#22A5F140",
		"list.focusForeground": "#FAFAFA",
		"list.inactiveSelectionBackground":"#22A5F140",
		"list.inactiveSelectionForeground":"#FAFAFA",
		"list.errorForeground":"#FF6665",
		"list.warningForeground":"#FFE391",
		"listFilterWidget.background":"#1A1A1A",
		"listFilterWidget.outline":"#808080",
		"listFilterWidget.noMatchesOutline":"#FF6665",

		// Activity Bar:
		"activityBar.background": "#1A1A1A",
		"activityBar.foreground": "#FAFAFA",
		"activityBar.inactiveForeground":"#808080",
		"activityBarBadge.background":"#22A5F1",
		"activityBarBadge.foreground":"#FAFAFA",

		// Side Bar:
		"sideBar.background":"#1A1A1A",
		"sideBar.foreground":"#FAFAFA",
		"sideBarTitle.foreground":"#808080",
		"sideBarSectionHeader.background":"#1A1A1A",
		"sideBarSectionHeader.foreground": "#808080",

		// Editor Groups And Tabs:
		"editorGroupHeader.tabsBackground":"#1A1A1A",
		"tab.activeBackground":"#22A5F1",
		"tab.activeForeground":"#FAFAFA",
		"tab.inactiveBackground":"#1A1A1A",
		"tab.inactiveForeground":"#FAFAFA",
		"tab.border":"#808080",

		// Editor Colors:
		"editor.background":"#1A1A1A",
		"editorLineNumber.foreground":"#808080",
		"editorLineNumber.activeForeground":"#22A5F1",
		"editor.selectionBackground":"#22A5F140",

		//"editor.selectionHighlightBackground":"#22A5F1",
		"editor.selectionHighlightBorder":"#22A5F1",
		"editor.rangeHighlightBackground":"#22A5F140",
		"editorError.foreground":"#FF6665",

		// Diff Editor:
		"diffEditor.insertedTextBorder":"#22A5F1",
		"diffEditor.removedTextBorder":"#FF6665",
		"diffEditor.border":"#808080",
		"diffEditor.diagonalFill":"#1A1A1A",

		// Editor Widget Colors:
		"editorWidget.background":"#1A1A1A",
		"editorWidget.foreground":"#808080",
		"editorSuggestWidget.background":"#1A1A1A",

		// Peek View Colors:
		"peekViewEditor.background":"#1A1A1A",
		"peekViewResult.background":"#1A1A1A",
		"peekViewEditor.matchHighlightBackground":"#22A5F140",
		"peekView.border":"#808080",

		// Merge Conflicts:
		"merge.currentHeaderBackground":"#22A5F1",
		"merge.currentContentBackground":"#FAFAFA",
		"merge.incomingHeaderBackground":"#FF6665",
		"merge.incomingContentBackground":"#FAFAFA",
		"merge.border":"#808080",
		"merge.commonContentBackground":"#1A1A1A",
		"merge.commonHeaderBackground": "#1A1A1A",

		// Panel Colors:
		"panel.background":"#1A1A1A",
		"panel.border":"#1A1A1A",
		"panelTitle.activeBorder":"#22A5F1",
		"panelTitle.inactiveForeground":"#808080",
		"panelTitle.activeForeground":"#FAFAFA",
		"panelInput.border":"#808080",

		// Status Bar Colors:
		"statusBar.background":"#1A1A1A",
		"statusBar.foreground":"#808080",
		"statusBar.border":"#1A1A1A",
		"statusBar.debuggingForeground":"#22A5F1",
		"statusBarItem.prominentForeground":"#FAFAFA",
		"statusBarItem.hoverBackground":"#22A5F140",
		"statusBarItem.remoteBackground":"#22A5F1",
		"statusBarItem.remoteForeground":"#FAFAFA",

		// Title Bar:
		"titleBar.inactiveBackground":"#1A1A1A",
		"titleBar.inactiveForeground": "#808080",
		"titleBar.activeBackground": "#1A1A1A",
		"titleBar.activeForeground": "#808080",

		// Menu Bar:
		"menu.background": "#1A1A1A",
		"menu.foreground": "#808080",
		"menubar.selectionBackground":"#22A5F140",
		"menubar.selectionForeground":"#FAFAFA",
		"menu.separatorBackground":"#22A5F1",
		"menu.selectionBackground":"#22A5F140",
		"menu.selectionForeground": "#FAFAFA",

		// Notification Colors:
		"notifications.background":"#1A1A1A",
		"notifications.foreground":"#808080",
		"notificationLink.foreground":"#22A5F1",
		"notificationsErrorIcon.foreground":"#FF6665",
		"notificationsWarningIcon.foreground":"#FFE391",
		"notificationsInfoIcon.foreground":"#22A5F1",

		// Extensions:
		"extensionButton.prominentForeground":"#FAFAFA",
		"extensionButton.prominentBackground":"#22a5f1",
		"extensionButton.prominentHoverBackground":"#22A5F140",
		"extensionBadge.remoteBackground":"#22A5F1",
		"extensionBadge.remoteForeground": "#FAFAFA",

		// Quick Picker:
		"quickInput.background":"#1A1A1A",
		"quickInput.foreground":"#808080",
		"pickerGroup.border":"#22A5F1",
		"pickerGroup.foreground":"#FAFAFA",

		// Integreted Terminal:
		"terminal.ansiGreen":"#61EBB4",
		"terminal.ansiMagenta":"#A684EB",
		"terminal.ansiYellow":"#FFE391",
		"terminal.ansiCyan":"#22A5F1",

		// Git Colors:
		"gitDecoration.addedResourceForeground":"#22A5F1",
		"gitDecoration.ignoredResourceForeground":"#808080",
		"gitDecoration.modifiedResourceForeground":"#FFE391",
		"gitDecoration.untrackedResourceForeground":"#7DE8BE",

		// Settings Editor Colors:
		"settings.headerForeground":"#22A5F1",
		"settings.modifiedItemIndicator":"#FFE391",
		"settings.checkboxBorder":"#22A5F1",
		"settings.dropdownForeground":"#FAFAFA",
		"settings.textInputForeground":"#FAFAFA",
		"settings.numberInputForeground":"#FAFAFA",

		// Breadcrumbs:
		"breadcrumb.background": "#1A1A1A",
		"breadcrumb.foreground": "#808080"
	},
	
	"tokenColors": [
		{
			"name": "Comment",
			"scope":[
				"comment",
				"comment.block",
				"comment.block.documentation",
				"comment.line",
			],
			"settings": {
				"foreground": "#808080",
				"fontStyle": "italic"
			}
		},

		/* -- JavaScript -- */

		{
			"name": "Operators",
			"scope":[
				"keyword.operator.assignment.js",
				"keyword.operator.arithmetic.js",
				"keyword.operator.bitwise.js",
				"keyword.operator.relational.js",
				"keyword.operator.logical.js",
				"keyword.operator.comparison.js",
				"storage.type.function.arrow.js",
				"keyword.operator.accessor.js",
				"keyword.operator.spread.js",
				"keyword.operator.js",
				"source.js",
				"keyword.operator.decrement.js",
				"keyword.operator.increment.js",
			],
			"settings": {
				"foreground": "#E8547C",
			}
		},

		{
			"name": "White",
			"scope":[
				"punctuation.definition.string.begin.js",
				"punctuation.definition.string.end.js",
				"meta.delimiter.comma.js",
				"punctuation.terminator.statement.js",
				"punctuation.definition.quasi.begin.js",
				"punctuation.definition.quasi.end.js",
				"punctuation.separator.key-value.js",
			],
			"settings": {
				"foreground": "#FAFAFA",
			}
		},

		{
			"name": "Purple",
			"scope":[
				"storage.type.js",
				"storage.type.function.js",
				"keyword.control.flow.js",
				"keyword.control.trycatch.js",
				"storage.type.class.js",
				"storage.type.extends.js",
				"variable.language.this.js",
				"keyword.control.switch.js",
				"keyword.control.loop.js",
				"keyword.control.module.js",
				"keyword.control.conditional.js",
			],
			"settings": {
				"foreground": "#A684EB",
			},
		},

		{
			"name": "Light Yellow",
			"scope":[
				"variable",
				"entity.name.class.js",
				"constant.other.object.key.js",
				"support.type.object.console.js"
			],
			"settings": {
				"foreground": "#FFE391",
			}
		},

		{
			"name": "Blue",
			"scope":[
				"entity.name.function.js",
				"entity.name.type.instance.js",
				"support.function.mutator.js",
				"punctuation.quasi.element.begin.js",
				"punctuation.quasi.element.end.js",
			],
			"settings": {
				"foreground": "#22A5F1",
			}
		},

		{
			"name": "Light Green",
			"scope":[
				"entity.name.function.method.js",
				"support.function.console.js",
				"variable.other.object.js",
				"support.class.builtin.js"
			],
			"settings": {
				"foreground": "#46E4A5",
			}
		},

		{
			"name": "New",
			"scope":[
				"keyword.operator.new.js",
			],
			"settings": {
				"foreground": "#F07567",
			}
		},

		{
			"name": "Grey",
			"scope":[
				"string.quoted.double.js",
				"string.quoted.single.js",
				"string.quoted.module.js",
				"string.quoted.template.js",
			],
			"settings": {
				"foreground": "#A5B6C4",
				"fontStyle": "italic"
			}
		},

		{
			"name": "Numeric",
			"scope":[
				"constant.numeric.js",
				"constant.language.boolean.true.js"
			],
			"settings": {
				"foreground": "#22A5F1",
			}
		},

		{
			"name": "Light Orange",
			"scope":[
				"variable.other.property.js"
			],
			"settings": {
				"foreground": "#FFAB82",
			}
		},

		/* -- Golang -- */

		{
			"name": "Purple",
			"scope":[
				"keyword.package.go",
				"keyword.function.go",
				"keyword.control.go",
				"keyword.type.go",
				"keyword.struct.go",
				"keyword.import.go",
				"keyword.var.go",
				"keyword.interface.go",
				"keyword.const.go",
				"keyword.map.go",
				"keyword.channel.go",
			],
			"settings": {
				"foreground": "#A684EB",
			}
		},

		{
			"name": "Yellow",
			"scope":[
				"entity.name.package.go",
				"entity.name.type.go",
				"source.go"
			],
			"settings": {
				"foreground": "#FFE391",
			}
		},

		{
			"name": "Blue",
			"scope":[
				"entity.name.function.go",
				"storage.type.numeric.go",
				"storage.type.string.go",
				"constant.language.go",
				"constant.numeric.integer.go",
				"constant.numeric.floating-point.go",
			],
			"settings": {
				"foreground": "#22A5F1",
			}
		},

		{
			"name": "Dark Pink",
			"scope":[
				"keyword.operator.comparison.go",
				"keyword.operator.assignment.go",
				"keyword.operator.logical.go",
				"keyword.operator.increment.go",
				"keyword.operator.decrement.go",
				"punctuation.other.period.go",
				"keyword.operator.address.go",
				"keyword.operator.arithmetic.go",
				"punctuation.other.colon.go",
			],
			"settings": {
				"foreground": "#E8547C",
			}
		},

		{
			"name": "Grey",
			"scope":[
				"entity.name.import.go",
				"constant.other.rune.go",
				"string.quoted.double.go",
			],
			"settings": {
				"foreground": "#A5B6C4",
				"fontStyle": "italic"
			}
		},

		{
			"name": "Green",
			"scope":[
				"support.function.builtin.go",
				"support.function.go",
			],
			"settings": {
				"foreground": "#46E4A5",
			}
		},

		{
			"name": "White",
			"scope":[
				"punctuation.definition.string.begin.go",
				"punctuation.definition.string.end.go",
				"punctuation.terminator.go",
				"punctuation.other.comma.go"
			],
			"settings": {
				"foreground": "#FAFAFA",
			}
		},
	]
}